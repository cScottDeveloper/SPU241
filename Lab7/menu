echo "0: Exit"
echo "1: Show Date and Time"
echo "2: Display Calendar"
echo "3: Call My Script"
echo "4: Call C Program"
echo
echo -n "Enter Your Choice: "
read option
echo
case $option in
   0) echo Good Bye ;;
   1) date 
      echo ;;
   2) cal ;;
   3) ./spell_check_2 my_dictionary date.c ;;
   4) ./hello ;;
   *) echo "Invlaid Input. Bye." ;;
esac
echo "this menu program is over"
